#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/a/MERN Stack/TalentForge/node_modules/@antfu/ni/bin/node_modules:/mnt/a/MERN Stack/TalentForge/node_modules/@antfu/ni/node_modules:/mnt/a/MERN Stack/TalentForge/node_modules/@antfu/node_modules:/mnt/a/MERN Stack/TalentForge/node_modules:/mnt/a/MERN Stack/node_modules:/mnt/a/node_modules:/mnt/a/MERN Stack/TalentForge/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/a/MERN Stack/TalentForge/node_modules/@antfu/ni/bin/node_modules:/mnt/a/MERN Stack/TalentForge/node_modules/@antfu/ni/node_modules:/mnt/a/MERN Stack/TalentForge/node_modules/@antfu/node_modules:/mnt/a/MERN Stack/TalentForge/node_modules:/mnt/a/MERN Stack/node_modules:/mnt/a/node_modules:/mnt/a/MERN Stack/TalentForge/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@antfu/ni/bin/nci.mjs" "$@"
else
  exec node  "$basedir/../@antfu/ni/bin/nci.mjs" "$@"
fi
